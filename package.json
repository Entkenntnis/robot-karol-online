{
  "name": "robot-karol-online",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "start": "vite preview",
    "grammar": "lezer-generator ./lib/codemirror/parser/karol.grammar -o ./lib/codemirror/parser/parser.js",
    "grammar-java": "lezer-generator ./lib/codemirror/javaParser/java.grammar -o ./lib/codemirror/javaParser/parser.js",
    "grammar-python": "lezer-generator ./lib/codemirror/pythonParser/python.grammar -o ./lib/codemirror/pythonParser/parser.js",
    "pack": "nodemon --watch lib/data/chapters --ext \"*\" scripts/pack.js",
    "update-gallery": "node scripts/generate-dictionary.js",
    "tauri": "tauri"
  },
  "dependencies": {
    "@blockly/field-slider": "^8.0.1",
    "@codemirror/autocomplete": "^6.0.0",
    "@codemirror/commands": "^6.0.0",
    "@codemirror/language": "^6.0.0",
    "@codemirror/lint": "^6.0.0",
    "@codemirror/search": "^6.2.3",
    "@codemirror/state": "^6.0.0",
    "@codemirror/view": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@lezer/common": "^1.2.0",
    "@lezer/highlight": "^1.2.0",
    "blockly": "^12.0.0",
    "canvas-confetti": "^1.6.0",
    "clsx": "^2.0.0",
    "date-fns": "^4.0.0",
    "fastest-levenshtein": "^1.0.16",
    "immer": "^10.0.0",
    "lodash.throttle": "^4.1.1",
    "markdown-to-jsx": "^7.7.6",
    "pullstate": "^2.0.0-pre.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-measure": "^2.5.2",
    "socket.io-client": "^4.8.1",
    "timeago-react": "^3.0.7",
    "tone": "^15.1.22"
  },
  "devDependencies": {
    "@lezer/generator": "^1.7.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tauri-apps/cli": "^2.4.0",
    "@types/canvas-confetti": "^1.6.0",
    "@types/node": "^24.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.49",
    "prettier": "3.8.1",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.8.3",
    "vite": "^7.3.1"
  },
  "overrides": {
    "jsdom": "npm:dry-uninstall@0.3.0"
  }
}
